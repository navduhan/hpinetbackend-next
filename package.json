{
  "name": "hpinetbackend-next",
  "version": "1.0.0",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "check": "node --check src/server.js",
    "snapshot:build": "node scripts/build-plant-snapshots.js",
    "db:indexes": "node scripts/create-mongo-indexes.js",
    "db:indexes:dry": "node scripts/create-mongo-indexes.js --dry-run",
    "results:cleanup": "node scripts/cleanup-old-results.js --days 30",
    "results:cleanup:dry": "node scripts/cleanup-old-results.js --days 30 --dry-run",
    "update": "git pull origin main && npm i && pm2 restart hpinetbackend-next",
    "test": "npm run check"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "mongoose": "^8.10.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
