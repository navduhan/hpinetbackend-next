{
  "apps": [
    {
      "name": "hpinetbackend-next",
      "cwd": "/home/labadmin/apps/hpinetdb/hpinetdbbackend-next",
      "script": "npm",
      "args": "start",
      "exec_mode": "fork",
      "instances": 1,
      "autorestart": true,
      "max_restarts": 20,
      "restart_delay": 5000,
      "max_memory_restart": "1200M",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3815",
        "MONGODB_URI": "mongodb://127.0.0.1:27017/default",
        "INTEROLOG_MONGO_DB": "hpinetdb",
        "CORS_ENABLED": "true",
        "CORS_ALLOWED_ORIGINS": "https://kaabil.net,https://bioinfo.usu.edu"
      }
    },
    {
      "name": "hpinet-results-cleanup",
      "cwd": "/home/labadmin/apps/hpinetdb/hpinetdbbackend-next",
      "script": "npm",
      "args": "run results:cleanup",
      "exec_mode": "fork",
      "instances": 1,
      "autorestart": false,
      "cron_restart": "30 3 * * *",
      "env": {
        "NODE_ENV": "production",
        "MONGODB_URI": "mongodb://127.0.0.1:27017/default"
      }
    }
  ]
}
