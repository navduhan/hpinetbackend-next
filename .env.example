NODE_ENV=production
PORT=3815
MONGODB_URI=mongodb://127.0.0.1:27017/default
INTEROLOG_MONGO_DB=hpinetdb

# If your reverse proxy already adds CORS headers, set this to false.
CORS_ENABLED=true
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000,https://kaabil.net

# Request handling
REQUEST_TIMEOUT_MS=600000

# Runtime data paths for phylogeny features.
# Update if your phylo assets are stored elsewhere.
PHYLO_ROOT=/home/labadmin/apps/hpinetdb/hpinetdbbackend-next/data/phylo
PHYLO_TMP_ROOT=/home/labadmin/apps/hpinetdb/hpinetdbbackend-next/tmp/phylo-jobs
DIAMOND_BIN=/home/labadmin/miniconda3/envs/tools/bin/diamond
PHYLO_THREADS=6

# JS GO similarity runtime
GO_AUTO_DOWNLOAD_OBO=true
GO_OBO_URL=https://purl.obolibrary.org/obo/go/go-basic.obo
GO_OBO_PATH=/home/labadmin/apps/hpinetdb/hpinetdbbackend-next/data/go/go-basic.obo
GO_MONGO_COLLECTION=go_terms_v2
